{
  "name": "dotkernel/dot-event",
  "type": "library",
  "description": "DotKernel event component extending and customizing zend-eventmanager",
  "license": "MIT",
  "authors": [
    {
      "name": "n3vrax",
      "email": "tibi@apidemia.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-servicemanager": "^3.3.0",
    "zendframework/zend-eventmanager": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.8",
    "squizlabs/php_codesniffer": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Dot\\Event\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DotTest\\Event\\": "test/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1-dev",
      "dev-develop": "0.2-dev"
    }
  }
}
